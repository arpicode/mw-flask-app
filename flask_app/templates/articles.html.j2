{% extends "layout.html.j2" %}
{% set active_page = "articles" %}

{% block title %}Articles{% endblock %}

{% block content %}
<h1>Articles Page</h1>
<ul class="list-group">
    {% for article in articles %}
    <li class="list-group-item"><a href="articles/{{article.id}}">{{article.title}}</a></li>
    {% endfor %}
</ul>
{% endblock %}
